{
  "manifest_version": 3,
  "name": "HRT",
  "description": "HRT's Test Extension",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "runtime"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.amazon.com/*"], 
      "js": ["content.js"]
    }
  ]  
}